{
  "name": "quiz-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "mysql2": "^3.10.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider && react-scripts --openssl-legacy-provider start",
